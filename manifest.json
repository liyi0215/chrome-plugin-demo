{
    "name": "make-noInterest-obvious",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "让百度首页【不感兴趣】按钮显示更明显",
    "icons": {
        "16": "image/icon16.png",
        "48": "image/icon48.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "js": ["js/jquery.min.js", "js/index.js"],
        "matches": ["*://www.baidu.com/"]
    }],
    "permissions": [
        "*://*.baidu.com/",
        "webRequest",
        "webRequestBlocking",
        "tabs"
    ]
}